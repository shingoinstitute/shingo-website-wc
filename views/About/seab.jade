extends ../layout 

block content
    ul(class="profile-pic-list")
        each member in members
            li(class="card h-50 p-4" style="width: 14rem;")
                img(src="#{member.Photograph__c}" class="profile-pic")
                div(class="staff")
                    h3= member.Name
                    h4= member.Title
                    h4= member.Account.Name
                a(href="/about/seab/" + member.Id)
                    p About >>
                    
                    
                    
                    
                