extends ../layout 

block content
    a(href="/about/seab")
        img(src="https://img.icons8.com/color/65/000000/circled-left-2.png")
    div(class="profile-image")
        img(src="#{member.Photograph__c}" class="profile-pic profile-pic-large")
    div(class="profile")
        h2= member.Name
        h3= member.Title
        h3= member.Account.Name
    div(class="biography")
        p !{member.Biography__c}
                    