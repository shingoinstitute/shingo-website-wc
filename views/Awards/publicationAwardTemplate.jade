extends ../layout 

block content
    a(href="/awards/publication")
        img(src="https://img.icons8.com/color/65/000000/circled-left-2.png")
    div(class="publication-award")
        h1= award.Name
        h3 Author: #{award.Public_Author_Name__c}
        if award.Cover_Image__c
            img(src="#{award.Cover_Image__c}")
        a(href="#{award.Order_Url__c}" target="_blank")
            p ORDER NOW
        p !{award.Press_Release_Copy__c}
